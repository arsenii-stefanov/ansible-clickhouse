#jinja2:lstrip_blocks: True
<?xml version="1.0"?>
<!-- {{ ansible_managed }} -->
<yandex>
  <compression>
{% if clickhouse_compression is defined and clickhouse_compression != "" %}
{% for item in clickhouse_compression %}
    <case>
      <min_part_size>{{ item.min_part_size }}</min_part_size>
      <min_part_size_ratio>{{ item.min_part_size_ratio }}</min_part_size_ratio>
      <method>{{ item.method }}</method>
    </case>
{% endfor %}
{% endif %}
  </compression>
</yandex>